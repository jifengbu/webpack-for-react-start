{
  "name": "SLPolice_PC",
  "version": "1.0.0",
  "scripts": {
    "dist": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --config ./webpack/prod/webpack.config.js  --progress",
    "app": "cross-env NODE_ENV=production webpack --config ./webpack/app/webpack.config.js  --progress",
    "start": "cd dist && cross-env NODE_ENV=production node app",
    "startDev": "cd build && cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 node app",
    "dev": "parallelshell \"npm run webpackServer\" \"npm run watchServer\"",
    "watchServer": "nodemon -e js,jsx,json -w App/server -w App/shared/routers -x \"webpack --config ./webpack/dev/webpack.node.config.js && npm run startDev\"",
    "webpackServer": "webpack-dev-server --config ./webpack/dev/webpack.browser.config.js --inline --hot --quiet --display-error-details",
    "lint": "eslint --ext .jsx,.js App",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "connect-mongo": "^3.2.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.1",
    "express-useragent": "^1.0.15",
    "graphql": "^15.5.0",
    "graphql-type-json": "^0.3.2",
    "html2canvas": "^1.0.0-rc.7",
    "invariant": "^2.2.4",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nzh": "^1.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "redux-combine-actions": "^0.1.2",
    "redux-logger": "^3.0.6",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "serialize-javascript": "^5.0.1",
    "socket.io-client": "^3.1.0",
    "superagent": "^6.1.0",
    "warning": "^4.0.3"
  },
